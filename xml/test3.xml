<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<expression xmlns:expression="file://schema/expression.xsd" type="arith">
    <operation type="/">
        <operation type="*">
            <operand>2</operand>
            <operand>3</operand>
        </operation>
        <operation type="-">
            <operand>2</operand>
            <operation type="~">
                <operation type="*">
                    <operand>4</operand>
                    <operation type="~">
                        <operand>5</operand>
                    </operation>
                </operation>
            </operation>
        </operation>
    </operation>
</expression>
